{
  "name": "@botbuildercommunity/adapter-alexa",
  "version": "0.1.0",
  "description": "Alexa adapter for use with the bot framework",
  "main": "./lib/index.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "check-coverage": true,
    "lines": 0,
    "statements": 0,
    "functions": 0,
    "branches": 0,
    "exclude": [
      "**/*.d.ts",
      "**/*.test.js"
    ],
    "reporter": [
      "text",
      "cobertura",
      "html"
    ],
    "report-dir": "./.nyc_output/coverage"
  },
  "mocha": {
    "require": "ts-node/register",
    "spec": "./test/**/*.test.ts"
  },
  "scripts": {
    "clean": "rimraf ./lib",
    "build": "tsc",
    "test": "mocha",
    "test-ci": "nyc mocha --reporter xunit --reporter-options output=./mocha-test-results.xml --reporter-options suiteName=$npm_package_name"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BotBuilderCommunity/botbuilder-community-js.git"
  },
  "keywords": [
    "chatbots",
    "bot",
    "framework",
    "alexa",
    "alexa",
    "adapter",
    "adapters"
  ],
  "author": "Jacob Mayer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BotBuilderCommunity/botbuilder-community-js/issues"
  },
  "homepage": "https://github.com/BotBuilderCommunity/botbuilder-community-js#readme",
  "devDependencies": {
    "@types/mocha": "^5.2.7",
    "@types/node": "^13.1.0",
    "@types/qs": "^6.5.3",
    "@types/uuid": "^3.4.6",
    "mocha": "^6.2.2",
    "nyc": "^15.0.0",
    "rimraf": "^3.0.0",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.4"
  },
  "dependencies": {
    "ask-sdk-core": "^2.7.0",
    "ask-sdk-model": "^1.25.1",
    "botbuilder": "^4.7.0",
    "qs": "^6.9.1"
  }
}
